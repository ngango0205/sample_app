<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <div class ="logo">
      <%= link_to t(".title3") ,"#" %>
    </div>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to t(".title"), root_path %></li>
        <li><%= link_to t(".title1"), help_path %></li>
        <% if logged_in? %>
        <li><%= link_to t(".title4"), "#"%></li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <%= t(".title8") %><b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to t(".title5"), current_user %></li>
              <li><%= link_to t(".title6"), "#"%></li>
              <li class="divider"></li>
              <li>
                <%= link_to t(".title7"), logout_path, method: :delete %>
              </li>
            </ul>
          </li>
        <% else %>
        <li><%= link_to t(".title2"), login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
